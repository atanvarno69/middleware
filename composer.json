{
  "name": "atanvarno/middleware",
  "type": "library",
  "description": "PSR-15 HTTP server request handler for middleware",
  "keywords": [
    "PSR-15", "server request handler", "middleware"
  ],
  "homepage": "https://github.com/atanvarno69/middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "atanvarno69",
      "homepage": "https://github.com/atanvarno69"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/http-factory": "^1.0.1",
    "psr/http-server-middleware": "^1.0.1"
  },
  "require-dev": {
    "atanvarno/build-tools": "^0.1.0",
    "phpunit/phpunit": "^9.5.10",
    "squizlabs/php_codesniffer": "^3.6.1",
    "vimeo/psalm": "^4.10.0"
  },
  "autoload": {
    "psr-4": {
      "Atanvarno\\Middleware\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Atanvarno\\Middleware\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
